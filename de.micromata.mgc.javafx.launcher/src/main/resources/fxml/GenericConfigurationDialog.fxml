<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.micromata.mgc.javafx.launcher.gui.generic.GenericConfigurationDialog">
   <children>
      <VBox>
         <children>
            <TabPane fx:id="configurationTabs" prefHeight="500.0" prefWidth="800.0" tabClosingPolicy="UNAVAILABLE" />
            <HBox alignment="CENTER_RIGHT" spacing="10.0">
               <children>
                  <Button defaultButton="true" mnemonicParsing="false" onAction="#onSave" text="Save" />
            
                        <Button cancelButton="true" mnemonicParsing="false" onAction="#onCancel" text="Cancel" />
               </children>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </VBox.margin>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
