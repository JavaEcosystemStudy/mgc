<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane fx:id="mainAnchorPane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.micromata.mgc.javafx.launcher.gui.generic.GenericConfigurationDialog">
   <children>
      <AnchorPane fx:id="mainPane" prefHeight="200.0" prefWidth="200.0">
         <children>
            <TabPane fx:id="configurationTabs" maxHeight="1.7976931348623157E308" prefHeight="500.0" prefWidth="800.0" tabClosingPolicy="UNAVAILABLE" />
         </children>
      </AnchorPane>
      <HBox fx:id="buttonPane" alignment="CENTER_RIGHT" spacing="10.0">
         <children>
            <Button defaultButton="true" mnemonicParsing="false" onAction="#onSave" text="Save" />
      
                        <Button cancelButton="true" mnemonicParsing="false" onAction="#onCancel" text="Cancel" />
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </HBox>
   </children>
</AnchorPane>
